{
    "name": "fittedin-backend",
    "version": "1.0.0",
    "description": "FittedIn Backend API",
    "main": "server.js",
    "scripts": {
        "start": "node server.js",
        "dev": "nodemon server.js",
        "test": "jest",
        "db:migrate": "npx sequelize-cli db:migrate",
        "db:seed": "npx sequelize-cli db:seed:all",
        "db:seed:faker": "node scripts/seedDatabase.js",
        "db:seed:clear": "SEED_CLEAR=true node scripts/seedDatabase.js",
        "test:auto-accept": "node scripts/testAutoAccept.js",
        "send:connections": "node scripts/sendConnectionsToUser.js",
        "generate:content": "node scripts/generateDynamicContent.js",
        "connect:real-to-fake": "node scripts/connectRealUsersToFakeUsers.js",
        "db:reset": "npx sequelize-cli db:migrate:undo:all && npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all",
        "pm2:start": "pm2 start ecosystem.config.js --env production",
        "pm2:stop": "pm2 stop fittedin-backend",
        "pm2:restart": "pm2 restart fittedin-backend",
        "pm2:delete": "pm2 delete fittedin-backend",
        "pm2:logs": "pm2 logs fittedin-backend",
        "pm2:monit": "pm2 monit",
        "verify:config": "node scripts/verifyProductionConfig.js"
    },
    "dependencies": {
        "aws-sdk": "^2.1500.0",
        "bcryptjs": "^2.4.3",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "express-rate-limit": "^7.1.5",
        "express-validator": "^7.0.1",
        "helmet": "^7.1.0",
        "jsonwebtoken": "^9.0.2",
        "pg": "^8.11.3",
        "pg-hstore": "^2.3.4",
        "sequelize": "^6.35.0"
    },
    "devDependencies": {
        "@faker-js/faker": "^10.1.0",
        "jest": "^29.7.0",
        "nodemon": "^3.0.2",
        "sequelize-cli": "^6.6.2",
        "supertest": "^6.3.3"
    },
    "keywords": [
        "fitness",
        "wellness",
        "social",
        "api"
    ],
    "author": "FittedIn Team",
    "license": "MIT"
}