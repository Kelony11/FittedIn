{
    "name": "fittedin-backend",
    "version": "1.0.0",
    "description": "FittedIn Backend API",
    "main": "server.js",
    "scripts": {
        "start": "node server.js",
        "dev": "nodemon server.js",
        "test": "jest",
        "db:migrate": "npx sequelize-cli db:migrate",
        "db:seed": "npx sequelize-cli db:seed:all",
        "db:reset": "npx sequelize-cli db:migrate:undo:all && npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all"
    },
    "dependencies": {
        "express": "^4.18.2",
        "cors": "^2.8.5",
        "helmet": "^7.1.0",
        "bcryptjs": "^2.4.3",
        "jsonwebtoken": "^9.0.2",
        "sequelize": "^6.35.0",
        "pg": "^8.11.3",
        "pg-hstore": "^2.3.4",
        "dotenv": "^16.3.1",
        "express-validator": "^7.0.1",
        "express-rate-limit": "^7.1.5"
    },
    "devDependencies": {
        "nodemon": "^3.0.2",
        "jest": "^29.7.0",
        "supertest": "^6.3.3",
        "sequelize-cli": "^6.6.2"
    },
    "keywords": [
        "fitness",
        "wellness",
        "social",
        "api"
    ],
    "author": "FittedIn Team",
    "license": "MIT"
}